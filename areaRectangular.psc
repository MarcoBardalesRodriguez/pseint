Algoritmo areaRectangular
	//definimos las variables
	Definir alto, largo, area Como Real;
	
	//solicitamos los datos
	Escribir 'altura del rectangulo';
	Leer alto;
	Escribir 'largo del rectangulo';
	Leer largo;
	
	//Proceso
	area = alto * largo;
	
	//Imprimir 
	Escribir 'el area es: ', area;
	
FinAlgoritmo
