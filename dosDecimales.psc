Algoritmo dosDecimales
	//Definir 
	Definir numInicial, numFinal Como Real;
	
	//Leer 
	Escribir 'reducir a dos decimales';
	Escribir 'ingrese un numero';
	Leer numInicial;
	
	//Proceso 
	numFinal =  (Redon(numInicial *  100)/100);
	
	//Imprimir 
	Escribir 'numero con dos decimales';
	Escribir numFinal;
	
FinAlgoritmo
